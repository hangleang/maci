{
  "name": "maci-subgraph",
  "type": "module",
  "description": "A subgraph to index data from MACI protocol to serve as data layer for frontend integration",
  "license": "MIT",
  "scripts": {
    "codegen:local": "node scripts/generateSubgraph.js localhost && graph codegen",
    "prebuild": "codegen:local",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ maci-subgraph",
    "create-local": "graph create --node http://localhost:8020/ maci-subgraph",
    "remove-local": "graph remove --node http://localhost:8020/ maci-subgraph",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 maci-subgraph",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.67.4",
    "@graphprotocol/graph-ts": "0.32.0"
  },
  "devDependencies": {
    "@types/mustache": "^4.2.5",
    "matchstick-as": "0.5.0",
    "mustache": "^4.2.0"
  }
}
